{
  "name": "eslint-plugin-slite",
  "version": "0.0.23",
  "description": "A set of rules used for code linting at @Slite",
  "main": "index.js",
  "license": "MIT",
  "author": "Slite Inc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/sliteteam/eslint-plugin-slite.git"
  },
  "scripts": {
    "lint": "eslint lib/ index.js",
    "test": "jest tests/"
  },
  "devDependencies": {
    "eslint": "^7.23.0",
    "jest": "^29.7.0"
  },
  "jest": {
    "projects": [
      {
        "displayName": "test",
        "roots": [
          "."
        ],
        "testRegex": "/tests/lib/rules/.+\\.jsx?$",
        "testEnvironment": "node"
      }
    ]
  }
}
